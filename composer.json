{
    "name": "vladimir-pejic/picophp",
    "description": "PicoPHP - Lightweight PHP framework heavily inspired by Laravel but not 0.01% as good, but whatever",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../picophp-core",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "vladimir-pejic/picophp-core": "@dev",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Vladimir Pejic",
            "email": "vladimir.pejic@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
